<%= form_tag '/session', method: :post do %>
    <%= label_tag "Username" %><br>
    <%= text_field_tag :username%><br><br><br>

    <%= label_tag "Password" %><br>
    <%= password_field_tag :password%><br><br><br>

    <%= submit_tag "Login" %>
<% end %>

<p>Or try logging in via <%= link_to 'GitHub', '/auth/github', method: :post %></p>